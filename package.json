{
  "name": "food",
  "version": "1.0.0",
  "description": "Food website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass SASS/main.scss CSS/Style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass SASS/main.scss CSS/Style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 version' CSS/style.comp.css -o  CSS/style.prifix.css",
    "compress:css": "node-sass css/style.prifix.css  css/style.css  --output-style compressed",
    "bulid:css": "npm-run-all compile:sass  prefix:css compress:css"
  },
  "author": "Hussein Alimi",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  }
}
